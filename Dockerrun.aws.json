{
    "AWSEBDockerrunVersion":2,
    "containerDefinitions":[
        {
            "name":"client",
            "image":"rossferreira/multi-client",
            "hostname": "client",
            "essential": false
        },
        {
            "name":"server",
            "image":"rossferreira/multi-server",
            "hostname": "api",
            "essential": false
        },
        {
            "name":"worker",
            "image":"rossferreira/multi-worker",
            "hostname": "worker",
            "essential": false
        },
        {
            "name":"nginx",
            "image":"rossferreira/multi-nginx",
            "essential": true,
            "portMappings":[
                {
                    "hostPort":80,
                    "containerPort":80
                }
            ],
            "links":["client","server"]
        }
    ]
}